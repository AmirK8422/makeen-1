{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/(site)/(URL)/shareoffice", "src/components/section/HomePage/articleSection/ArticleSection.tsx", "src/components/section/HomePage/Options.tsx", "src/components/section/HomePage/HomePage.jsx", "src/app/Admin/(URL)/Sections/Mentors/page.tsx"],
  "exclude": ["node_modules"]
}
