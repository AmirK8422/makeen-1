{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/(URLS)/shareoffice", "src/components/section/HomePage/article/Article.Sectiontsx", "src/components/section/HomePage/Opsions.tsx", "src/components/section/HomePage/HomePage.jsx", "src/app/admin/(URL)/Sections/Mentors/page.Øªsx"],
  "exclude": ["node_modules"]
}
